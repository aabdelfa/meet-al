<p class="navbar-text pull-right">
<% if admin_signed_in? %>
<div class="pull-left">
  <span> Welcome, <strong><%= current_admin.username %></strong>.</span>
  <%= link_to "Logout", destroy_admin_session_path, method: :delete, :class => 'navbar-link' %>
</div>
<br>
<% provide(:title, "Edit timeline") %>
<h1>Edit coding timeline information</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for @timeline, url: { action: '' }  do |f| %>

      <%= f.label :user %>
      <%= f.text_field :user, default: 'Alaa', disabled: true, class: 'form-control' %>
      <br>
      <%= f.label :image %>
      <%= f.text_field :image, default: nil, class: 'form-control' %>
      <br>
      <%= f.label :content %>
      <%= f.text_field :content, class: 'form-control' %>
      <br><br>
      <%= f.submit "Save", class: "btn btn-primary" %> <%= link_to 'Cancel', 'index', class: 'btn btn-default' %>
    <% end %>
  </div>
</div>
<% end %>